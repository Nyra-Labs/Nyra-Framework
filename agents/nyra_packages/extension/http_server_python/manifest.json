{
  "type": "extension",
  "name": "http_server_python",
  "version": "0.1.0",
  "dependencies": [
    {
      "type": "system",
      "name": "nyra_runtime_python",
      "version": "0.6"
    }
  ],
  "package": {
    "include": [
      "manifest.json",
      "property.json",
      "**.py"
    ]
  },
  "api": {
    "property": {
      "lisnyra_addr": {
        "type": "string"
      },
      "lisnyra_port": {
        "type": "int32"
      }
    },
    "cmd_out": [
      {
        "name": "update_querying_collection",
        "property": {
          "filename": {
            "type": "string"
          },
          "collection": {
            "type": "string"
          }
        },
        "required": [
          "filename",
          "collection"
        ]
      },
      {
        "name": "file_chunk",
        "property": {
          "filename": {
            "type": "string"
          },
          "path": {
            "type": "string"
          },
          "collection": {
            "type": "string"
          }
        },
        "required": [
          "path"
        ]
      }
    ]
  }
}